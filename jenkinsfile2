pipeline {
   agent any
   tools {
      maven 'Maven'
   }
   stages {
       stage("build") {
                steps {
               
                    echo "Building" 
                   
                   
                }
       }
      stage("test") {
           steps {
               
               echo "Testing"
             
           }
          
        }
    
      stage("deploy") {
             steps{
                  echo "deploy in prod"
         
              }
      }      
      
  }
}
